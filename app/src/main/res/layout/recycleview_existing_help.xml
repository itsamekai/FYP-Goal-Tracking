<?xml version="1.0" encoding="utf-8"?>
<androidx.cardview.widget.CardView xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:cardView="http://schemas.android.com/apk/res-auto"
    android:id="@+id/cardView8"
    android:layout_width="match_parent"
    android:layout_height="220dp"
    cardView:cardCornerRadius="15dp"
    cardView:cardElevation="8dp"
    android:layout_margin="8dp">
    <androidx.constraintlayout.widget.ConstraintLayout

        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <TextView
            android:id="@+id/existingGoalName"
            android:layout_width="200dp"
            android:layout_height="24dp"
            android:fontFamily="@font/poppins"
            android:text="TextView"
            android:textColor="@color/black"
            android:textSize="17sp"
            android:textStyle="bold"
            cardView:layout_constraintBottom_toBottomOf="parent"
            cardView:layout_constraintEnd_toEndOf="parent"
            cardView:layout_constraintHorizontal_bias="0.09"
            cardView:layout_constraintStart_toStartOf="parent"
            cardView:layout_constraintTop_toTopOf="parent"
            cardView:layout_constraintVertical_bias="0.142" />

        <TextView
            android:id="@+id/existingGoalDesc"
            android:layout_width="250dp"
            android:layout_height="25dp"
            android:layout_marginTop="4dp"
            android:fontFamily="@font/poppins"
            android:text="TextView"
            android:textColor="#696969"
            android:textSize="13sp"
            android:textStyle="italic"
            cardView:layout_constraintBottom_toBottomOf="parent"
            cardView:layout_constraintEnd_toEndOf="@+id/existingGoalName"
            cardView:layout_constraintHorizontal_bias="0.0"
            cardView:layout_constraintStart_toStartOf="@+id/existingGoalName"
            cardView:layout_constraintTop_toBottomOf="@+id/existingGoalName"
            cardView:layout_constraintVertical_bias="0.0" />

        <TextView
            android:id="@+id/existingDateTime"
            android:layout_width="250dp"
            android:layout_height="25dp"
            android:fontFamily="@font/poppins"
            android:text="TextView"
            android:textColor="@color/black"
            android:textSize="16sp"
            cardView:layout_constraintBottom_toBottomOf="parent"
            cardView:layout_constraintEnd_toEndOf="@+id/existingGoalDesc"
            cardView:layout_constraintHorizontal_bias="0.0"
            cardView:layout_constraintStart_toStartOf="@+id/existingGoalDesc"
            cardView:layout_constraintTop_toBottomOf="@+id/existingGoalDesc"
            cardView:layout_constraintVertical_bias="0.0" />

        <TextView
            android:id="@+id/existingOrgName"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="16dp"
            android:fontFamily="@font/poppins"
            android:text="TextView"
            android:textColor="@color/black"
            android:textSize="16sp"
            cardView:layout_constraintBottom_toBottomOf="parent"
            cardView:layout_constraintEnd_toEndOf="@+id/existingDateTime"
            cardView:layout_constraintHorizontal_bias="0.0"
            cardView:layout_constraintStart_toStartOf="@+id/existingDateTime"
            cardView:layout_constraintTop_toBottomOf="@+id/existingDateTime"
            cardView:layout_constraintVertical_bias="0.042" />

        <TextView
            android:id="@+id/existingOrgPhone"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginBottom="24dp"
            android:fontFamily="@font/poppins"
            android:text="TextView"
            android:textColor="@color/black"
            android:textSize="16sp"
            cardView:layout_constraintBottom_toBottomOf="parent"
            cardView:layout_constraintEnd_toEndOf="@+id/existingOrgName"
            cardView:layout_constraintHorizontal_bias="0.0"
            cardView:layout_constraintStart_toStartOf="@+id/existingOrgName" />
    </androidx.constraintlayout.widget.ConstraintLayout>
</androidx.cardview.widget.CardView>