<?xml version="1.0" encoding="utf-8"?>
<androidx.cardview.widget.CardView xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:cardView="http://schemas.android.com/apk/res-auto"
    android:id="@+id/cardView5"
    android:layout_width="match_parent"
    android:layout_height="160dp"
    cardView:cardCornerRadius="15dp"
    cardView:cardElevation="20dp"
    android:layout_margin="10dp">

    <androidx.constraintlayout.widget.ConstraintLayout

        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <TextView
            android:id="@+id/accomplishedAchievement"
            android:layout_width="200dp"
            android:layout_height="30dp"
            android:layout_marginBottom="20dp"
            android:fontFamily="@font/poppins"
            android:scrollHorizontally="false"
            android:text="TextView"
            android:textColor="@color/black"
            android:textSize="12sp"
            android:textStyle="bold"
            cardView:layout_constraintBottom_toTopOf="@+id/accomplishedDateTime"
            cardView:layout_constraintEnd_toEndOf="@+id/accomplishedDateTime"
            cardView:layout_constraintHorizontal_bias="0.0"
            cardView:layout_constraintStart_toStartOf="@+id/accomplishedDateTime"
            cardView:layout_constraintTop_toTopOf="parent"
            cardView:layout_constraintVertical_bias="0.363" />

        <TextView
            android:id="@+id/accomplishedDesc1"
            android:layout_width="250dp"
            android:layout_height="30dp"
            android:layout_marginBottom="8dp"
            android:fontFamily="@font/poppins"
            android:scrollHorizontally="false"
            android:text="TextView"
            android:textColor="@color/black"
            android:textSize="12sp"
            android:textStyle="bold"
            cardView:layout_constraintBottom_toTopOf="@+id/accomplishedDateTime"
            cardView:layout_constraintEnd_toEndOf="@+id/accomplishedDateTime"
            cardView:layout_constraintHorizontal_bias="1.0"
            cardView:layout_constraintStart_toStartOf="@+id/accomplishedDateTime"
            cardView:layout_constraintTop_toBottomOf="@+id/accomplishedAchievement"
            cardView:layout_constraintVertical_bias="0.833" />

        <TextView
            android:id="@+id/accomplishedDateTime"
            android:layout_width="250dp"
            android:layout_height="30dp"
            android:layout_marginBottom="24dp"
            android:fontFamily="@font/poppins"
            android:scrollHorizontally="false"
            android:text="TextView"
            android:textColor="@color/black"
            android:textSize="12sp"
            android:textStyle="bold"
            cardView:layout_constraintBottom_toBottomOf="parent"
            cardView:layout_constraintEnd_toEndOf="parent"
            cardView:layout_constraintHorizontal_bias="0.103"
            cardView:layout_constraintStart_toStartOf="parent" />

        <ImageView
            android:id="@+id/imageView10"
            android:layout_width="90dp"
            android:layout_height="76dp"
            cardView:layout_constraintBottom_toBottomOf="parent"
            cardView:layout_constraintEnd_toEndOf="parent"
            cardView:layout_constraintHorizontal_bias="0.947"
            cardView:layout_constraintStart_toStartOf="parent"
            cardView:layout_constraintTop_toTopOf="parent"
            cardView:srcCompat="@drawable/achievementmedal" />


    </androidx.constraintlayout.widget.ConstraintLayout>
</androidx.cardview.widget.CardView>