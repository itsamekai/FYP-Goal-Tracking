<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".GeneralMainPage.ProfilePageEdit"
    android:background="@drawable/editprofilebackgroud" >

    <ImageView
        android:id="@+id/ProfilePic"
        android:layout_width="131dp"
        android:layout_height="123dp"
        android:adjustViewBounds="true"
        android:contentDescription="@string/profilePicture"
        android:scaleType="fitXY"
        app:background="@drawable/border"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.026"
        app:shapeAppearanceOverlay="@style/Circular"
        app:srcCompat="@drawable/user1" />

    <ImageView
        android:id="@+id/Done"
        android:layout_width="42dp"
        android:layout_height="22dp"
        android:adjustViewBounds="true"
        android:contentDescription="Done_Button"
        android:scaleType="fitXY"
        app:background="@drawable/border"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.956"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.306"
        app:shapeAppearanceOverlay="@style/Circular"
        app:srcCompat="@drawable/donebutton" />

    <TextView
        android:id="@+id/user_fullname"
        android:layout_width="184dp"
        android:layout_height="46dp"
        android:fontFamily="@font/segoe"
        android:gravity="center"
        android:text="User Name"
        android:textSize="25sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.497"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.202" />

    <EditText
        android:id="@+id/aboutdetails"
        android:layout_width="306dp"
        android:layout_height="39dp"
        android:fontFamily="@font/segoe"
        android:gravity="center"
        android:text="73-year-old swag kid"
        android:textSize="15sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.495"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.445" />

    <TextView
