<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Register.OrgRegisterPage"
    android:background="@drawable/purple_background">

    <ImageView
        android:id="@+id/ReturnArrow3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:contentDescription="@string/returnPrev"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.088"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.044"
        app:srcCompat="@drawable/arrow" />

    <TextView
        android:id="@+id/textView4"
        android:layout_width="257dp"
        android:layout_height="49dp"
        android:fontFamily="@font/segoe"
        android:gravity="center"
        android:text="@string/org_register_string"
        android:textColor="@color/white"
        android:textSize="25sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.148" />

    <EditText
        android:id="@+id/orgName"
        android:layout_width="275dp"
        android:layout_height="50dp"
        android:layout_marginTop="25dp"
        android:background="@drawable/login_shape"
        android:ems="10"
        android:fontFamily="@font/segoe"
        android:hint="@string/org_name"
        android:importantForAutofill="no"
        android:inputType="textPersonName"
        android:drawableLeft="@drawable/hierarchy"
        android:drawableStart="@drawable/hierarchy"
        android:drawablePadding="5dp"
        android:paddingLeft="10dp"
        android:paddingRight="10dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView4"
        app:layout_constraintVertical_bias="0.0" />

    <EditText
        android:id="@+id/orgEmailAddress"
        android:layout_width="275dp"
        android:layout_height="50dp"
        android:layout_marginTop="20dp"
        android:background="@drawable/login_shape"
        android:drawableStart="@drawable/email"
        android:drawableLeft="@drawable/email"
        android:drawablePadding="10dp"
        android:ems="10"
        android:fontFamily="@font/segoe"
        android:hint="@string/email_address"
        android:importantForAutofill="no"
        android:inputType="textEmailAddress"
        android:paddingLeft="10dp"
        android:paddingRight="10dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/orgName"
        app:layout_constraintVertical_bias="0.002" />

    <EditText
        android:id="@+id/orgPassword"
        android:layout_width="275dp"
        android:layout_height="50dp"
        android:layout_marginTop="20dp"
        android:background="@drawable/login_shape"
        android:drawableStart="@drawable/padlock"
        android:drawableLeft="@drawable/padlock"
        android:drawablePadding="10dp"
        android:ems="10"
        android:fontFamily="@font/segoe"
        android:hint="@string/orgLogin_password"
        android:importantForAutofill="no"
        android:inputType="textPassword"
        android:paddingLeft="7dp"
        android:paddingRight="7dp"
        app:layout_constraintBottom_toTopOf="@+id/orgPhoneNo"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/orgEmailAddress"
        app:layout_constraintVertical_bias="0.0" />

    <EditText
        android:id="@+id/orgPhoneNo"
        android:layout_width="275dp"
        android:layout_height="50dp"
        android:layout_marginTop="20dp"
        android:background="@drawable/login_shape"
        android:drawableStart="@drawable/phone_call"
        android:drawableLeft="@drawable/phone_call"
        android:drawablePadding="10dp"
        android:ems="10"
        android:fontFamily="@font/segoe"
        android:hint="@string/orgPhoneNo"
        android:importantForAutofill="no"
        android:inputType="phone"
        android:paddingLeft="7dp"
        android:paddingRight="7dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/orgEmailAddress"
        app:layout_constraintVertical_bias="0.141" />


    <EditText
        android:id="@+id/orgInCharge"
        android:layout_width="275dp"
        android:layout_height="50dp"
        android:layout_marginTop="20dp"
        android:background="@drawable/login_shape"
        android:drawableStart="@drawable/user2"
        android:drawableLeft="@drawable/user2"
        android:drawablePadding="10dp"
        android:ems="10"
        android:fontFamily="@font/segoe"
        android:hint="@string/inCharge"
        android:importantForAutofill="no"
        android:inputType="textPersonName"
        android:paddingLeft="10dp"
        android:paddingRight="10dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/orgPhoneNo"
        app:layout_constraintVertical_bias="0.0" />

    <EditText
        android:id="@+id/orgAdd"
        android:layout_width="275dp"
        android:layout_height="50dp"
        android:layout_marginTop="20dp"
        android:background="@drawable/login_shape"
        android:drawableStart="@drawable/home"
        android:drawableLeft="@drawable/home"
        android:drawablePadding="7dp"
        android:ems="10"
        android:fontFamily="@font/segoe"
        android:hint="@string/orgAddress"
        android:importantForAutofill="no"
        android:inputType="textPersonName"
        android:paddingLeft="5dp"
        android:paddingRight="5dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/orgInCharge"
        app:layout_constraintVertical_bias="0.0" />

    <Button
        android:id="@+id/orgRegisterButton"
        android:layout_width="135dp"
        android:layout_height="55dp"
        android:background="@drawable/login_shape"
        android:fontFamily="@font/segoe"
        android:text="@string/orgRegisterButton"
        android:textColor="@color/purple_background_color"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/orgAdd"
        app:layout_constraintVertical_bias="0.141" />




</androidx.constraintlayout.widget.ConstraintLayout>