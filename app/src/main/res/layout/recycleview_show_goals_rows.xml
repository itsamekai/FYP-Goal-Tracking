<?xml version="1.0" encoding="utf-8"?>
<androidx.cardview.widget.CardView
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:cardView="http://schemas.android.com/apk/res-auto"
    android:id="@+id/cardView5"
    android:layout_width="match_parent"
    android:layout_height="160dp"
    cardView:cardCornerRadius="15dp"
    cardView:cardElevation="20dp"
    android:layout_margin="10dp">

    <androidx.constraintlayout.widget.ConstraintLayout

        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <TextView
            android:id="@+id/viewGoalUser"
            android:layout_width="220dp"
            android:layout_height="30dp"
            android:fontFamily="@font/poppins"
            android:gravity="start"
            android:text="TextView"
            android:textColor="@color/black"
            android:textSize="16sp"
            android:textStyle="bold"
            cardView:layout_constraintBottom_toBottomOf="parent"
            cardView:layout_constraintEnd_toEndOf="parent"
            cardView:layout_constraintHorizontal_bias="0.093"
            cardView:layout_constraintStart_toStartOf="parent"
            cardView:layout_constraintTop_toTopOf="parent"
            cardView:layout_constraintVertical_bias="0.24000001" />

        <TextView
            android:id="@+id/viewGoalCategory"
            android:layout_width="220dp"
            android:layout_height="30dp"
            android:fontFamily="@font/poppins"
            android:gravity="start"
            android:text="TextView"
            android:textColor="@color/black"
            android:textSize="16sp"
            android:textStyle="bold"
            cardView:layout_constraintBottom_toBottomOf="parent"
            cardView:layout_constraintEnd_toEndOf="parent"
            cardView:layout_constraintHorizontal_bias="0.093"
            cardView:layout_constraintStart_toStartOf="parent"
            cardView:layout_constraintTop_toTopOf="parent"
            cardView:layout_constraintVertical_bias="0.52" />

        <TextView
            android:id="@+id/viewGoalName"
            android:layout_width="220dp"
            android:layout_height="50dp"
            android:fontFamily="@font/poppins"
            android:gravity="start"
            android:scrollHorizontally="false"
            android:text="TextView"
            android:textColor="@color/black"
            android:textSize="16sp"
            android:textStyle="bold"
            cardView:layout_constraintBottom_toBottomOf="parent"
            cardView:layout_constraintEnd_toEndOf="parent"
            cardView:layout_constraintHorizontal_bias="0.093"
            cardView:layout_constraintStart_toStartOf="parent"
            cardView:layout_constraintTop_toTopOf="parent"
            cardView:layout_constraintVertical_bias="0.945" />

        <TextView
            android:id="@+id/textView31"
            android:layout_width="109dp"
            android:layout_height="26dp"
            android:fontFamily="@font/poppins"
            android:gravity="center"
            android:text="Accomplished:"
            android:textColor="@color/black"
            android:textSize="15sp"
            android:textStyle="bold"
            cardView:layout_constraintBottom_toTopOf="@+id/Accomplished"
            cardView:layout_constraintEnd_toEndOf="parent"
            cardView:layout_constraintHorizontal_bias="0.86"
            cardView:layout_constraintStart_toStartOf="parent"
            cardView:layout_constraintTop_toTopOf="parent"
            cardView:layout_constraintVertical_bias="0.822" />

        <TextView
            android:id="@+id/Accomplished"
            android:layout_width="72dp"
            android:layout_height="40dp"
            android:layout_marginBottom="32dp"
            android:fontFamily="@font/poppins"
            android:gravity="center"
            android:text="TextView"
            android:textColor="@color/black"
            android:textStyle="bold"
            cardView:layout_constraintBottom_toBottomOf="parent"
            cardView:layout_constraintEnd_toEndOf="parent"
            cardView:layout_constraintHorizontal_bias="0.81"
            cardView:layout_constraintStart_toStartOf="parent" />

    </androidx.constraintlayout.widget.ConstraintLayout>
</androidx.cardview.widget.CardView>